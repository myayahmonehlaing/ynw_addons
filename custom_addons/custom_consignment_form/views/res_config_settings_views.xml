<odoo>
  <record id="res_config_settings_view_form_inherit_consignment" model="ir.ui.view">
    <field name="name">res.config.settings.inherit.consignment</field>
    <field name="model">res.config.settings</field>
    <field name="inherit_id" ref="sale.res_config_settings_view_form"/>
    <field name="arch" type="xml">
      <xpath expr="//block[@name='sale_shipping_setting_container']/setting[last()]" position="after">
        <setting id="consignment_warehouse_setting" string="Consignment Warehouse"
                 help="Select default warehouse to use for consignment stock transfers.">
          <field name="consignment_warehouse_id"/>
        </setting>
      </xpath>
    </field>
  </record>
</odoo>

